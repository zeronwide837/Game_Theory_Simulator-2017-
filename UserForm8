Private Sub Label1_Click()
Dim Y, S, YH As Integer

Y = Int((Rnd * 100)) + 1
S = Int((Rnd * 100)) + 1
YH = Int((Rnd * 100)) + 1

If S > 70 Then
Image2.Visible = False
Image4.Visible = False
Image6.Visible = True

If YH > 30 Then
Image7.Visible = True

Else
Image1.Visible = False
Image3.Visible = False
Image5.Visible = True

End If

Else
Image1.Visible = False
Image3.Visible = False
Image5.Visible = True

If YH > 30 Then
If S > 70 Then
Do


Loop While YH > 30 And S > 70
Else
Image7.Visible = True
End If

Else
Image2.Visible = False
Image4.Visible = False
Image6.Visible = True
End If

End If

End Sub

